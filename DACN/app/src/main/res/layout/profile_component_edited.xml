<?xml version="1.0" encoding="UTF-8" ?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    style="@style/background"
    tools:context=".MainActivity"
    android:gravity="start">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:ignore="UselessParent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="16dp"
            style="@style/profile_component"
            android:orientation="vertical"
            android:fadingEdgeLength="10dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView2">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="25dp"
                android:layout_marginRight="25dp"
                android:layout_marginTop="20dp"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/imageViewMail"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.173"
                    app:srcCompat="@drawable/mail" />
                <EditText
                    android:id="@+id/profile_component_edited_page_email"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:autofillHints="profile_component_edited_page_email"
                    android:hint="@string/profile_component_edited_page_email"
                    android:background="@drawable/no_underline_for_edt"
                    style="@style/text"
                    android:layout_marginStart="50dp"
                    android:layout_gravity="center_vertical"
                    android:textSize="14sp" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="25dp"
                android:layout_marginRight="25dp"
                android:layout_marginTop="10dp"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/imageViewDateofBirth"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.173"
                    android:background="@drawable/cake" />
                <EditText
                    android:id="@+id/profile_component_edited_page_date_of_birth"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:hint="@string/profile_component_edited_page_date_of_birth"
                    android:background="@drawable/no_underline_for_edt"
                    style="@style/text"
                    android:layout_marginStart="50dp"
                    android:layout_gravity="center_vertical"
                    android:textSize="14sp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="25dp"
                android:layout_marginRight="25dp"
                android:layout_marginTop="10dp"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/imageView4"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.173"
                    app:srcCompat="@drawable/gender" />
                <EditText
                    android:id="@+id/profile_component_edited_page_gender"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:hint="@string/profile_component_edited_page_gender"
                    android:background="@drawable/no_underline_for_edt"
                    style="@style/text"
                    android:layout_marginStart="50dp"
                    android:layout_gravity="center_vertical"
                    android:textSize="14sp" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="25dp"
                android:layout_marginRight="25dp"
                android:layout_marginTop="10dp"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/imageView9"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.173"
                    app:srcCompat="@drawable/assert_phone" />
                <EditText
                    android:id="@+id/profile_component_edited_page_phone_number"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:hint="@string/profile_component_edited_page_phone_number"
                    android:background="@drawable/no_underline_for_edt"
                    style="@style/text"
                    android:layout_marginStart="50dp"
                    android:layout_gravity="center_vertical"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="25dp"
                android:layout_marginTop="10dp"
                android:layout_marginRight="25dp"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageView3"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.173"
                    app:srcCompat="@drawable/assert_location" />

                <EditText
                    android:id="@+id/profile_component_edited_page_address"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:background="@drawable/no_underline_for_edt"
                    android:hint="@string/profile_component_edited_page_address"
                    android:layout_marginStart="50dp"
                    style="@style/text"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="25dp"
                android:layout_marginRight="25dp"
                android:layout_marginTop="10dp"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/imageView5"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.173"
                    app:srcCompat="@drawable/idetitycard" />
                <EditText
                    android:id="@+id/profile_component_edited_page_idcard"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:hint="@string/profile_component_edited_page_idcard"
                    android:background="@drawable/no_underline_for_edt"
                    style="@style/text"
                    android:layout_marginStart="50dp"
                    android:layout_gravity="center_vertical"
                    android:textSize="14sp" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="25dp"
                android:layout_marginRight="25dp"
                android:layout_marginTop="10dp"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/imageView7"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.173"
                    app:srcCompat="@drawable/national" />
                <EditText
                    android:id="@+id/profile_component_edited_page_nationality"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:hint="@string/profile_component_edited_page_nationality"
                    android:background="@drawable/no_underline_for_edt"
                    style="@style/text"
                    android:layout_marginStart="50dp"
                    android:layout_gravity="center_vertical"
                    android:textSize="14sp" />
                <EditText
                    android:id="@+id/profile_component_edited_page_national"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:hint="@string/profile_component_edited_page_national"
                    android:background="@drawable/no_underline_for_edt"
                    style="@style/text"
                    android:layout_marginStart="50dp"
                    android:layout_gravity="center_vertical"
                    android:textSize="14sp" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="25dp"
                android:layout_marginRight="25dp"
                android:layout_marginTop="5dp"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/imageView8"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.173"
                    app:srcCompat="@drawable/foreign" />
                <EditText
                    android:id="@+id/profile_component_edited_page_language_proficiency"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:hint="@string/profile_component_edited_page_language_proficiency"
                    android:background="@drawable/no_underline_for_edt"
                    style="@style/text"
                    android:layout_marginStart="50dp"
                    android:layout_gravity="center_vertical"
                    android:textSize="14sp" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="25dp"
                android:layout_marginRight="25dp"
                android:layout_marginTop="10dp"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/imageView10"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.173"
                    app:srcCompat="@drawable/day" />
                <EditText
                    android:id="@+id/profile_component_edited_page_start_date"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:hint="@string/profile_component_edited_page_start_date"
                    android:background="@drawable/no_underline_for_edt"
                    style="@style/text"
                    android:layout_marginStart="50dp"
                    android:layout_gravity="center_vertical"
                    android:textSize="14sp" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="25dp"
                android:layout_marginRight="25dp"
                android:layout_marginTop="10dp"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/imageView11"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.173"
                    app:srcCompat="@drawable/department" />
                <EditText
                    android:id="@+id/profile_component_edited_page_department"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:hint="@string/profile_component_edited_page_department"
                    android:background="@drawable/no_underline_for_edt"
                    style="@style/text"
                    android:layout_marginStart="50dp"
                    android:layout_gravity="center_vertical"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="25dp"
                android:layout_marginRight="25dp"
                android:layout_marginTop="10dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/profile_component_edited_page_Status"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:hint="@string/profile_component_edited_page_Status"
                    style="@style/text"
                    android:layout_marginStart="80dp"
                    android:layout_gravity="center_vertical"
                    android:textSize="14sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50dp"
                android:layout_marginRight="50dp"
                android:layout_marginTop="30dp"
                android:gravity="center"
                android:orientation="horizontal">
                <Button
                    android:id="@+id/profile_component_edited_page_save_btn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:paddingHorizontal="55dp"
                    android:text="@string/profile_component_edited_page_save_btn"
                    android:textSize="14sp"
                    style="@style/button_text" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginLeft="50dp"
                android:layout_marginRight="50dp"
                android:layout_marginTop="5dp"
                android:gravity="center"
                android:orientation="horizontal"/>
        </LinearLayout>
    </ScrollView>
</RelativeLayout>