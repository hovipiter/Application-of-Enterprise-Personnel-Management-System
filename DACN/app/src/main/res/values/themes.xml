<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.DACN" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
    </style>

    <style name="background">
        <item name="android:background">@color/white</item>
    </style>
    <!--    Button-->
    <style name="button">
        <item name="android:backgroundTint">@color/teal</item>
    </style>
    <!--    text-->
    <style name="text">
        <item name="android:textColor">@color/black</item>
    </style>
    <!--    text + button-->
    <style name="button_text">
        <item name="android:textColor">@color/black</item>
        <item name="android:backgroundTint">@color/teal</item>
    </style>

    <style name="drawable_assert">
        <item name="android:background">@drawable/rounded_corner</item>
    </style>

    <style name="drawable_assert_and_text">
        <item name="android:background">@drawable/rounded_corner</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="image_find_dark">
        <item name="android:src">@drawable/search</item>
        <item name="android:backgroundTint">@color/white</item>
    </style>

    <style name="background_banner_home">
        <item name="android:background">@drawable/background_banner</item>
    </style>

    <style name="banner_night">
        <item name="android:src">@drawable/banner</item>
    </style>

    <style name="profile_component">
        <item name="android:background">@drawable/bottom_gb_for_user_page</item>
    </style>

<!--    chamcong_camera_page-->



</resources>